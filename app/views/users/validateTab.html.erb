<div class="center jumbotron">
  <h1>Validate your Data</h1>
  <div class="row">
  <div class="col-md-6 col-md-offset-3">

<div class="row">
  <div class="col-md-12 col-md-offset-0">
    <%= form_for(@user) do |f| %>
      <div class = "orb">
      <%= f.label :Table_To_Backup %>
	    <%= f.select :t , @result.rows ,:required => true%> 

      <%= f.label :Checksum_File_To_Use%>
      <%= f.file_field :dir, :required => true, :accept=>".txt"%> 
      </div>
  </div>
      <%= f.submit "Submit",name:"validate_tab" ,class: "btn btn-primary" %>
    <% end %>
  </div>
</div>

  </div>
</div>